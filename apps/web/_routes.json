{
  "version": 1,
  "include": ["/*"],
  "exclude": [
    "/_next/static/*",
    "/static/*",
    "/*.ico",
    "/*.json",
    "/*.png",
    "/*.svg",
    "/cache/**/*"
  ],
  "routes": [
    {
      "src": "/api/.*",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
} 